{
  "rewrites": [{ "source": "/(.*)", "destination": "/api" }],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "src/index.js",
          "methods": [
              "GET",
              "POST",
              "PUT",
              "PATCH",
              "OPTIONS",
              "DELETE",
              "HEAD",
              "CONNECT",
              "TRACE"
          ]
      }
  ]
}
